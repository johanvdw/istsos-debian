                  <swe:uom code="iso8601"/>
                </swe:Time>
              </swe:field>
              <swe:field name="air humidity">
                <swe:Quantity definition="urn:ogc:def:parameter:x-istsos:1.0:meteo:air:humidity" gml:id="2">
                  <swe:uom code="%"/>
                </swe:Quantity>
              </swe:field>
            </swe:DataRecord>
          </swe:elementType>
          <swe:encoding>
            <swe:TextBlock blockSeparator="@" decimalSeparator="." tokenSeparator=","/>
          </swe:encoding>
          <swe:values/>
